{
    "manifest_version": 2,
    "name": "Bodhi_Background",
    "description": "Adding a background to Google.com",
    "version": "1.0",
    "icons": {
        "16": "/Images/icon-16px.jpg",
        "48": "/Images/icon-48px.jpg",
        "128": "/Images/icon-128px.jpg"
    },
    "page_action": {
        "default_icon": {
            "16": "/Images/icon-16px.jpg",
            "48": "/Images/icon-48px.jpg",
            "128": "/Images/icon-128px.jpg"
        }
    },
    "content_scripts": [{
        "matches": ["https://www.google.com/*"],
        "css": ["style.css"]





    }]


}